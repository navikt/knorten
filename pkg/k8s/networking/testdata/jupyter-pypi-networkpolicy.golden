apiVersion: networking.k8s.io/v1
kind: NetworkPolicy
metadata:
  labels:
    managed-by: knorten.knada.io
  name: jupyter-policy
  namespace: test-namespace
spec:
  egress:
  - ports:
    - port: 443
      protocol: TCP
    to:
    - ipBlock:
        cidr: 151.101.0.0/16
  podSelector:
    matchLabels:
      app: jupyterhub
      component: singleuser-server
  policyTypes:
  - Egress
